{
  "kind": "collectionType",
  "collectionName": "owners",
  "info": {
    "name": "Owner"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "password": {
      "type": "password",
      "required": true,
      "maxLength": 64,
      "minLength": 8,
      "private": true
    },
    "registered_at": {
      "type": "datetime"
    }
  }
}
